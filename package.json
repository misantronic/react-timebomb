{
  "name": "react-timebomb",
  "version": "5.1.3",
  "description": "yet another date- and timepicker based on typescript, react and styled-components.",
  "main": "./dist/index.js",
  "scripts": {
    "compile": "rm -rf dist && tsc -p . && node fuse && cp dist/index.d.ts dist/react-timebomb.d.ts && cp dist/index.d.ts dist/react-timebomb.min.d.ts",
    "examples": "rm -rf .cache && parcel examples/src/index.html --out-dir examples/dist",
    "publish": "git tag -a $npm_package_version -m 'new version' && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/misantronic/react-timebomb.git"
  },
  "author": "David Schkalee <david.schkalee@posteo.de.de>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/misantronic/react-timebomb/issues"
  },
  "homepage": "https://github.com/misantronic/react-timebomb#readme",
  "peerDependencies": {
    "react": ">= 16.0.0",
    "react-dom": ">= 16.0.0"
  },
  "dependencies": {
    "moment": ">= 2.x",
    "react-slct": ">= 2.0.0",
    "react-with-gesture": "^4.0.2",
    "styled-components": ">= 4.1.0"
  },
  "devDependencies": {
    "@types/react": "^16.8.1",
    "@types/react-dom": "^16.0.x",
    "@types/styled-components": "^4.1.x",
    "fuse-box": "3.7.0-next.6",
    "parcel-bundler": "^1.9.6",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "typescript": "^3.3.x"
  }
}
